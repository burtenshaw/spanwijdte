  file = {/home/ben/Documents/research/zotero_files/storage/7VGJFQ7J/Ger et al_2007_Adaptive Units of Learning and Educational Videogames.pdf;/home/ben/Documents/research/zotero_files/storage/QM5Z6GDN/2007-5.html},
  journal = {Journal of Interactive Media in Education},
  keywords = {<e-Adventure>,adaptable units of learning,e-learning,IMS learning design},
  language = {en},
  number = {1}
}

