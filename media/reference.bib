@article{ger_adaptive_2007,
  title = {Adaptive {{Units}} of {{Learning}} and {{Educational Videogames}}},
  author = {Ger, Pablo Moreno and Thomas, Pilar Sancho and Ortiz, Iv{\'a}n Mart{\'i}nez and Sierra, Jos{\'e} and Manj{\'o}n, Baltasar Fern{\'a}ndez},
  year = {2007},
  month = sep,
  volume = {2007},
  issn = {1365-893X},
  doi = {10.5334/2007-5},
  abstract = {In this paper, we propose three different ways of using IMS Learning Design to support online adaptive learning modules that include educational videogames. The first approach relies on IMS LD to support adaptation procedures where the educational games are considered as Learning Objects. These games can be included instead of traditional content to adapt the learning experience to the preferences of the student. In the second approach, the game engine supports the entire adaptation cycle, simply using IMS LD as a delivery mechanism for the packages containing the games. The learner profile is used to adapt the game, and the activity within the game is used to adjust the profile. Finally, the third approach stands between the other two, using IMS LD to model the general adaptation guidelines and then refining the adaptation inside the games. We illustrate these approaches with the \&lt;e-Adventure\&gt; educational game engine, which includes the mechanisms required to support these adaptation cycles.Editors: Daniel Burgos.},
