@article{vanbruwaeneMultiplatformDatasetDetecting2020,
  ids = {vanbruwaeneMultiplatformDatasetDetecting2020a},
  title = {A Multi-Platform Dataset for Detecting Cyberbullying in Social Media},
  author = {Van Bruwaene, David and Huang, Qianjia and Inkpen, Diana},
  year = {2020},
