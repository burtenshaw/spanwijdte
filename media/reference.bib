@article{hayles_complexities_2002,
  title = {The {{Complexities}} of {{Seriation}}},
  author = {Hayles, N. Katherine},
  year = {2002},
  volume = {117},
  pages = {117--121},
  issn = {0030-8129},
  doi = {10.1632/003081202X63564},
  abstract = {GLOBALIZATION US ALL INTO THE POSITION OF THE BLIND MEN EXAMINING IS SUCH A LARGE CONCEPT THAT IT FORCES AN elephant-able to articulate the part we touch but not able to grasp the whole. So it is with the provocative papers in this issue by Mark Poster, Bruce Robbins, and Thomas Keenan, along with Emily Apter's introduc- tion. Each makes excellent points but also mounts a different argument. As a group they present a more comprehensive view of globalization than they do when read separately, and I want to explore the shape that emerges from their interactions. I see my task less as weaving them into a seamless tapestry than as highlighting the ways in which they chal- lenge and complement one another. By pointing out the incompleteness as well as the accomplishment of each, I want to show that together they suggest new interactions between the local and the global and, through these interactions, new possibilities for political awareness and action},
